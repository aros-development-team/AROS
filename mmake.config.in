[AROS]
# You need only uncomment the next line if you run mmake from somewhere
# else than $(TOP).
#top /home/digulla/AROS
maketool $(HOST_MAKE) $(MKARGS) TOP=$(TOP) CURDIR=$(CURDIR) TARGET=$(TARGET)
defaultmakefilename mmakefile
defaulttarget AROS
genmakefilescript $(GENMF) $(TOP)/config/make.tmpl < $(MMSRC) > $(MMDEST)
genmakefiledeps $(GENMF) $(TOP)/config/make.tmpl
globalvarfile $(TOP)/bin/$(AROS_HOST_ARCH)-$(AROS_HOST_CPU)/gen/config/host.cfg
genglobalvarfile sh $(TOP)/configure

ignoredir CVS
ignoredir bin
ignoredir intuition2

#uncomment next line if you have checked out contrib/regina


