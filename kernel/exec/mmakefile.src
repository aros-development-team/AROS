include $(TOP)/config/make.cfg

CFLAGS := $(CFLAGS) -D__KERNEL__

#MM setup-ppc : 
setup-ppc : exec_init.s
	@$(NOP)

#MM setup-prep-ppc :
setup-prep-ppc:
	%mkdirs_q $(BINDIR)/boot

$(OBJDIR)/%.o : %.S
	%assemble_q

$(OBJDIR)/%.o : %.c
	%compile_q

