# Makefile for loaddesktop program

include $(TOP)/config/make.cfg

FILES	:= loaddesktop

EXEDIR := $(AROSDIR)/test/desktop
EXE := $(EXEDIR)/loaddesktop

USE_CLIB_SHARED := yes
USE_AMIGALIB    := yes
USE_MUILIB      := yes
USE_AROSLIB     := yes

#MM test-desktop : test-desktop-setup includes linklibs

%make_prog_setup mmake=test-desktop

test-desktop : $(EXE)

%make_prog_single dir=$(EXEDIR)

#MM test-desktop-setup
%rule_makedirs dirs=$(EXEDIR) setuptarget=test-desktop-setup

%common
%include_deps
