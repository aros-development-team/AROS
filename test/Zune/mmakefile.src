# Makefile for Zune test programs

include $(TOP)/config/make.cfg

FILES  := test virttest gradient

EXEDIR := $(AROSDIR)/test/Zune
EXES   := $(foreach f,$(FILES),$(EXEDIR)/$(f))

USE_CLIB_SHARED := yes
USE_AMIGALIB    := yes
USE_MUILIB      := yes
USE_AROSLIB     := yes

#MM test-zune : includes linklibs

%make_prog_setup mmake=test-zune

test-zune : setup $(EXES)

%make_prog_single dir=$(EXEDIR)

#MM setup
setup ::
	%mkdirs_q $(EXEDIR)

%common
%include_deps
