# Makefile for Zune test programs

include $(TOP)/config/make.cfg
include ../make.cfg

FILES	:= test

EXEDIR := $(ZUNEDIR)/Tests
EXES := $(foreach f,$(FILES),$(EXEDIR)/$(f))

USE_CLIB_SHARED := yes
USE_AMIGALIB    := yes
USE_MUILIB      := yes
USE_AROSLIB     := yes

#MM contrib-zune-test : includes linklibs

%make_prog_setup mmake=contrib-zune-test

contrib-zune-test : setup $(EXES)

%make_prog_single dir=$(EXEDIR)

#MM setup
setup ::
	%mkdirs_q $(ZUNEDIR)
	%mkdirs_q $(ZUNEDIR)/Tests

%common
%include_deps
