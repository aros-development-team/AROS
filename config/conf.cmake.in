SET(CMAKE_SYSTEM_NAME AROS)
SET(CMAKE_SYSTEM_VERSION 1)
SET(BUILD_SHARED_LIBS OFF)
SET(LIBTYPE STATIC)
SET(CMAKE_BUILD_TYPE Release)
SET(CMAKE_FIND_ROOT_PATH @AROS_BUILDDIR@/bin/@aros_target_arch@-@aros_target_cpu@@aros_target_suffix@/AROS/Development)
SET(CMAKE_FIND_ROOT_PATH_MODE_PROGRAM NEVER)
SET(CMAKE_FIND_ROOT_PATH_MODE_INCLUDE ONLY)
SET(CMAKE_FIND_ROOT_PATH_MODE_LIBRARY ONLY)
SET(CMAKE_LIBRARY_PATH_FLAG @AROS_BUILDDIR@/bin/@aros_target_arch@-@aros_target_cpu@@aros_target_suffix@/AROS/Development/lib)
SET(CMAKE_SYSTEM_PROCESSOR @aros_target_cpu@)
SET(CMAKE_AR @aros_plain_ar@ CACHE FILEPATH "Archiver")
IF(NOT CMAKE_C_COMPILER)
  SET(CMAKE_C_COMPILER @aros_target_cc@)
  SET(CMAKE_C_COMPILER_WORKS:INTERNAL TRUE)
  # SET(CMAKE_C_FLAGS "@aros_target_cflags@" CACHE STRING "" FORCE)
ENDIF()
IF(NOT CMAKE_CXX_COMPILER)
  SET(CMAKE_CXX_COMPILER @aros_target_cxx@)
  SET(CMAKE_CXX_COMPILER_WORKS:INTERNAL TRUE)
  # SET(CMAKE_CXX_FLAGS "@aros_target_cxxflags@" CACHE STRING "" FORCE)
ENDIF()
SET(CMAKE_EXE_LINKER_FLAGS "" CACHE STRING "" FORCE)
SET(CMAKE_MODULE_PATH "@SRCDIR@/config/cmake" ${CMAKE_MODULE_PATH}) # For AROS FindPackage scripts
SET(CMAKE_BUILD_TYPE Release CACHE STRING "Debug|Release|RelWithDebInfo|MinSizeRel")
SET(CMAKE_CROSS_COMPILING ON) # Workaround for http://www.cmake.org/Bug/view.php?id=14075
SET(CMAKE_CROSSCOMPILING ON)
# SET(PKG_CONFIG_EXECUTABLE )
