out=bin/$aros_arch-$aros_cpu/AROS/include/aros/config.h
if cmp -s "$out" "config/config.h"; then
    echo "config/config.h is unchanged."
    rm $out
else
    echo "Replacing $out by config/config.h"
    if test -e $out ; then
	mv $out ${out}.bak
    fi
    mv config/config.h $out
fi
