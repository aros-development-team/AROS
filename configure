    if test "x$test_kernel_cc" != "xno"; then
        aros_target_incl=`$aros_kernel_cc -print-sysroot 2>/dev/null`/`$aros_kernel_cc --print-sysroot-headers-suffix 2>/dev/null || echo usr/include`
    else
        aros_target_incl=
    fi
