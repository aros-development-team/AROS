    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $target_gcc_version" >&5
printf "%s\n" "$target_gcc_version" >&6; }

    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking version string style" >&5
printf %s "checking version string style... " >&6; }
    if test "${target_gcc_version#*$'-'}" != "$target_gcc_version" ; then
        { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: snapshot" >&5
printf "%s\n" "snapshot" >&6; }
        GCC_VERSION_MAJOR=$(echo $target_gcc_version | cut -d'-' -f1)
        GCC_VERSION_MINOR=-1
        GCC_VERSION_PATCH=-1
    else
        { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: release" >&5
printf "%s\n" "release" >&6; }
        GCC_VERSION_MAJOR=$(echo $target_gcc_version | cut -d'.' -f1)
        GCC_VERSION_MINOR=$(echo $target_gcc_version | cut -d'.' -f2)
        GCC_VERSION_PATCH=$(echo $target_gcc_version | cut -d'.' -f3)
    fi

    # Check if there is a patch available for the target gcc version
    gcc_version_ok="no"
    supported_versions=""
    for patchfile in $(find "${SRCDIR}/tools/crosstools/gnu" -name 'gcc-*-aros.diff'); do
        patchbasename=$(basename "$patchfile")
        patchversion=$(echo "$patchbasename" | cut -d'-' -f2)
        supported_versions="$supported_versions $patchversion"
        if test "$patchversion" = "$target_gcc_version" ; then
            gcc_version_ok="yes"
            break
        fi
    done
    if test $gcc_version_ok = "no" ; then
        as_fn_error $? "GCC version $target_gcc_version is not supported. Supported versions are$supported_versions." "$LINENO" 5
    fi

    # from gcc 8, armv7-a+fp is used to specify
    # hard-float/vfpv3-d16
    arm_v7_flotattgt="armv7-a+fp"
    if test "$GCC_VERSION_MAJOR" -lt "10" ; then
        aros_config_cflags="$aros_config_cflags $""(CFLAGS_NO_COMMON)"
