		;;
	    *)
		{ echo "$as_me:$LINENO: WARNING: \"Unknown CPU for host -- $host_cpu\"" >&5
echo "$as_me: WARNING: \"Unknown CPU for host -- $host_cpu\"" >&2;}
		aros_host_cpu="$host_cpu"
		;;
	esac
        ;;

