# Find out if we are cross-compiling (i.e. if we can't use the host compiler
# for target code)
cross_compiling=no
if test "$aros_host_cpu" != "$aros_target_cpu" ; then
    cross_compiling=yes
fi

if test "$aros_host_cpu" == "x86_64" ; then
    if test "$aros_target_cpu" == "i386" ; then
        if test "$aros_target_arch" != "mingw32"; then
            cross_compiling=no
        fi
    fi
fi

if test "$aros_host_arch" == "darwin" ; then
    if test "$aros_target_arch" != "darwin" ; then
        cross_compiling=yes
    else
        cross_compiling=no
    fi
fi

if test "$aros_host_arch" == "mingw32" ; then
    if test "$aros_target_arch" != "mingw32" ; then
        cross_compiling=yes
    fi
fi

# I want to have this information in configure output, although it might be
# misleading, e.g. for darwin hosted you cannot compile target code without a
# cross compiler, but here it says no.

# Note: it says 'yes' only if bootstrap ("kernel") is cross-compiled.
# Target code is a little bit different thing.
#                                                sonic
echo "checking whether we are (AROS style) cross compiling... $cross_compiling"

# Set prefix for target compiler etc. if we're cross-compiling
# (should possibly be earlier but needs to be after AC_PROG_CC)
if test "$cross_compiling" = "yes" ; then

    if test "x-$aros_flavour" = "x-standalone" -o "x-$aros_flavour" = "x-standcompat" ; then
        if test "$aros_gcc_version" != "none"; then


                        target_tool_prefix=${target_cpu}-aros-
            target_tool_version=$aros_gcc_version
            use_kernel_cc_wrapper=yes

                                    if test "$crosstools" != yes; then
                aros_target_cc=
                aros_target_ld=
                aros_target_cpp=
                aros_target_as=
                aros_target_ar=
                aros_target_ranlib=
                aros_target_nm=
                aros_target_strip=
                aros_target_objcopy=
                aros_target_objdump=
            fi
        fi
    fi

    aros_kernel_cc=${kernel_tool_prefix}gcc
    if test "$kernel_tool_version" != "default"; then
        aros_kernel_cc="${aros_kernel_cc}-$kernel_tool_version"
    fi
fi

if test "$aros_kernel_cpp" = ""; then
    if test "$cross_compiling" = "yes" ; then
        # Extract the first word of "${kernel_tool_prefix}cpp", so it can be a program name with args.
set dummy ${kernel_tool_prefix}cpp; ac_word=$2
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
$as_echo_n "checking for $ac_word... " >&6; }
if ${ac_cv_path_aros_kernel_cpp+:} false; then :
  $as_echo_n "(cached) " >&6
else
  case $aros_kernel_cpp in
  [\\/]* | ?:[\\/]*)
  ac_cv_path_aros_kernel_cpp="$aros_kernel_cpp" # Let the user override the test with a path.
  ;;
  *)
  as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
    for ac_exec_ext in '' $ac_executable_extensions; do
  if { test -f "$as_dir/$ac_word$ac_exec_ext" && $as_test_x "$as_dir/$ac_word$ac_exec_ext"; }; then
    ac_cv_path_aros_kernel_cpp="$as_dir/$ac_word$ac_exec_ext"
    $as_echo "$as_me:${as_lineno-$LINENO}: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
  done
IFS=$as_save_IFS

  ;;
esac
fi
aros_kernel_cpp=$ac_cv_path_aros_kernel_cpp
if test -n "$aros_kernel_cpp"; then
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: $aros_kernel_cpp" >&5
$as_echo "$aros_kernel_cpp" >&6; }
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; }
fi


    else
        if test "$GCC" = "yes"; then
    aros_gcc_cpp=`$CC -print-prog-name=cpp`
    # Extract the first word of "`basename $aros_gcc_[cpp]`", so it can be a program name with args.
set dummy `basename $aros_gcc_cpp`; ac_word=$2
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for $ac_word" >&5
$as_echo_n "checking for $ac_word... " >&6; }
if ${ac_cv_path_aros_kernel_cpp+:} false; then :
  $as_echo_n "(cached) " >&6
else
  case $aros_kernel_cpp in
  [\\/]* | ?:[\\/]*)
  ac_cv_path_aros_kernel_cpp="$aros_kernel_cpp" # Let the user override the test with a path.
  ;;
  *)
  as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in `dirname $aros_gcc_cpp`
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
    for ac_exec_ext in '' $ac_executable_extensions; do
  if { test -f "$as_dir/$ac_word$ac_exec_ext" && $as_test_x "$as_dir/$ac_word$ac_exec_ext"; }; then
    ac_cv_path_aros_kernel_cpp="$as_dir/$ac_word$ac_exec_ext"
    $as_echo "$as_me:${as_lineno-$LINENO}: found $as_dir/$ac_word$ac_exec_ext" >&5
