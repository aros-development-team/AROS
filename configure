        case "$host_cpu" in
            *m68k*)
                aros_host_cpu="m68k"
                ;;
            *powerpc*)
                aros_host_cpu="ppc"
                ;;
            *)
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: \"Unknown CPU for host -- $host_cpu\"" >&5
$as_echo "$as_me: WARNING: \"Unknown CPU for host -- $host_cpu\"" >&2;}
                aros_host_cpu="$host_cpu"
                ;;
        esac
        ;;

    cygwin*)
        aros_host_arch="cygwin"
        use_aros_gcc="yes"
        aros_host_cc="$aros_host_cc $aros_host_cc_pipe"
        android_build_os="windows"
        android_tool="android.bat"
        default_android_sdk="/cygdrive/c/android-sdk-windows-1.6_r1"

        case "$host_cpu" in
            *i?86*)
                aros_host_cpu="i386"
                ;;
            *)
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: \"Unknown CPU for host -- $host_cpu\"" >&5
$as_echo "$as_me: WARNING: \"Unknown CPU for host -- $host_cpu\"" >&2;}
                aros_host_cpu="$host_cpu"
                ;;
        esac
        ;;

    mingw32*)
        aros_host_arch="mingw32"
        use_aros_gcc="yes"
        aros_host_cc="$aros_host_cc $aros_host_cc_pipe"
        android_build_os="windows"
        android_tool="android.bat"
        default_android_sdk="/c/android-sdk-windows-1.6_r1"

        case "$host_cpu" in
        *i?86*)

            { $as_echo "$as_me:${as_lineno-$LINENO}: checking for Windows native gcc target" >&5
$as_echo_n "checking for Windows native gcc target... " >&6; }
            host_cpu=`gcc -dumpmachine`
            { $as_echo "$as_me:${as_lineno-$LINENO}: result: $host_cpu" >&5
$as_echo "$host_cpu" >&6; }
            ;;

        esac

        case "$host_cpu" in
        *i?86*)
            aros_host_cpu="i386"
            ;;

        *x86_64*)
            aros_host_cpu="x86_64"
                                    aros_host_cflags="$aros_host_cflags -isystem /mingw/include"
            ;;

        *)
            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: \"Unknown CPU for host -- $host_cpu\"" >&5
$as_echo "$as_me: WARNING: \"Unknown CPU for host -- $host_cpu\"" >&2;}
            aros_host_cpu="$host_cpu"
            ;;
        esac
        ;;
    *)
        as_fn_error $? "Unsupported host architecture $host" "$LINENO" 5
        ;;
esac

