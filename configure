  if $as_executable_p "$as_dir/$ac_word$ac_exec_ext"; then
    ac_cv_path_TARGET_AS="$as_dir/$ac_word$ac_exec_ext"
    echo "$as_me:$LINENO: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
done

  test -z "$ac_cv_path_TARGET_AS" && ac_cv_path_TARGET_AS="$aros_target_as"
  ;;
esac
fi
