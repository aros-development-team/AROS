    if diff $ac_file "$tmp/config.h" >/dev/null 2>&1; then
      { echo "$as_me:$LINENO: $ac_file is unchanged" >&5
echo "$as_me: $ac_file is unchanged" >&6;}
    else
      rm -f $ac_file
      mv "$tmp/config.h" $ac_file
    fi
  else
    echo "/* $configure_input  */"
    cat "$ac_result"
  fi
  rm -f "$tmp/out12"
 ;;

  :C)  { echo "$as_me:$LINENO: executing $ac_file commands" >&5
echo "$as_me: executing $ac_file commands" >&6;}
 ;;
  esac


  case $ac_file$ac_mode in
    "compiler_executable":C)
        prefix=${PWD}/${aros_tools_dir}/${aros_target_cpu}-${aros_target_arch}${aros_target_suffix}
	mkdir -p $prefix
	prefix="${prefix}/${aros_target_cpu}-aros"
        prefix2=${PWD}/${aros_tools_dir}/${aros_target_cpu}-${aros_target_arch}${aros_target_suffix}-aros

	echo ${prefix}
	echo ${prefix2}
	chmod a+x ${prefix2}-gcc ${prefix2}-ld

	ln -sf ${prefix2}-gcc          ${prefix}-gcc
	ln -sf ${prefix2}-ld           ${prefix}-ld

        ln -sf $aros_target_as_ln      ${prefix}-as
        ln -sf $aros_target_nm_ln      ${prefix}-nm
        ln -sf $aros_target_ar_ln      ${prefix}-ar
        ln -sf $aros_target_ranlib_ln  ${prefix}-ranlib
        ln -sf $aros_target_objcopy_ln ${prefix}-objcopy
        ln -sf $aros_target_objdump_ln ${prefix}-objdump
        ln -sf $aros_target_strip_ln   ${prefix}-strip

        ln -sf $aros_target_as_ln      ${prefix2}-as
        ln -sf $aros_target_nm_ln      ${prefix2}-nm
        ln -sf $aros_target_ar_ln      ${prefix2}-ar
        ln -sf $aros_target_ranlib_ln  ${prefix2}-ranlib
        ln -sf $aros_target_objcopy_ln ${prefix2}-objcopy
        ln -sf $aros_target_objdump_ln ${prefix2}-objdump
        ln -sf $aros_target_strip_ln   ${prefix2}-strip
     ;;
    "genshared_executable":C) chmod a+x ${aros_scripts_dir}/genshared ;;
    "genmf_executable":C) chmod a+x ${aros_tools_dir}/genmf.py ;;
    "adflib_myaros.conf_executable":C) chmod a+x tools/adflib/myconf.aros
