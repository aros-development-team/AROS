# ---- UNIX board support package ----

KDEVS	:= 
KLIBS   := exec
KHNDLRS := 
KHIDDS  := 
KRSRCS  := 

%link_kickstart mmake=kernel-link-unix file=$(AROSDIR)/boot/aros-bsp-$(AROS_TARGET_ATRCH)$(AROS_TARGET_SUFFIX) \
	devs=$(KDEVS) handlers=$(KHNDLRS) hidds=$(KHIDDS) libs=$(KLIBS) res=$(KRSRCS) startup=$(KOBJSDIR)/kernel_resource.o \
	uselibs="hiddstubs"

%common
