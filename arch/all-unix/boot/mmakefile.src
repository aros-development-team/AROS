include $(TOP)/config/make.cfg

# -------- Bootstrap part --------

#MM boot-unix: kernel-bootstrap-hosted

#MM
boot-unix: $(AROSDIR)/boot/AROSBootstrap.conf

ifeq ($(AROS_TARGET_VARIANT),android))
$(AROSDIR)/boot/AROSBootstrap.conf: INPUT_FILE := $(SRCDIR)/$(CURDIR)/AROSBootstrap_android.conf
$(AROSDIR)/boot/AROSBootstrap.conf: AROSBootstrap_android.conf
else
$(AROSDIR)/boot/AROSBootstrap.conf: INPUT_FILE := $(SRCDIR)/$(CURDIR)/AROSBootstrap.conf
$(AROSDIR)/boot/AROSBootstrap.conf: AROSBootstrap.conf
endif
$(AROSDIR)/boot/AROSBootstrap.conf: mkbootconf.sh | $(AROSDIR)/boot
	@$(ECHO) "Writing    $@..."
	@$(SRCDIR)/$(CURDIR)/mkbootconf.sh $(INPUT_FILE) $(AROS_TARGET_ARCH)$(AROS_TARGET_SUFFIX) >$@

$(AROSDIR)/boot:
	@$(ECHO) "Making     $@..."
	@mkdir -p $(AROSDIR)/boot

