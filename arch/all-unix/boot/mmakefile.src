KDEVS	:= 
KLIBS   := exec
KHNDLRS := 
KHIDDS  := 
KRSRCS  := task

%link_kickstart mmake=kernel-link-unix file=$(AROSARCHDIR)/kernel \
	devs=$(KDEVS) handlers=$(KHNDLRS) hidds=$(KHIDDS) libs=$(KLIBS) res=$(KRSRCS) startup=$(KOBJSDIR)/kernel_resource.o \
	uselibs="hiddstubs"

%common
