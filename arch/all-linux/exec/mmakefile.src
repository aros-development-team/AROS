include $(TOP)/config/make.cfg

#MM setup-exec-linux : setup-exec-unix setup-exec-$(CPU)
setup-exec-linux : $(GENINCDIR)/sigcore.h

$(GENINCDIR)/sigcore.h : sigcore.h.src
	%mkdirs_q $(GENINCDIR)
	$(SHELL) ./mksigcore.sh $(TOP)/config/linux/exec $(GENINCDIR)/sigcore.h

OS_INCLUDES := -I$(SYS_INCLUDES) -I$(TOP)/rom/exec -I$(GENINCDIR)
%build_archspecific \
  mainmmake=kernel-exec maindir=rom/exec arch=linux \
  files=exec/init compiler=host
