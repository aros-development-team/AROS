timeout 5
default 9

title AROS64 with native Gfx
    kernel /boot/bootstrap-pc-x86_64 ATA=32bit
    module /boot/aros-bsp-pc-x86_64
    module /boot/aros-fs.pkg
    module /boot/aros-base.pkg
    module /boot/poseidon.pkg

title AROS64 with VESA Gfx @ 640x480-8bpp
