# $Id$
include $(TOP)/config/make.cfg

FILES		:= longjmp setjmp
CLIBDIR 	:= $(GENDIR)/compiler/clib
USER_CFLAGS	:= -I$(TOP)/compiler/clib -I..
CFLAGS2 	:= $(CFLAGS)
AFLAGS2 	:= $(AFLAGS) -I..
TESTS		:=

#MM
clib-pc-i386 : $(foreach f,$(FILES), $(CLIBDIR)/$(f).o)
	@$(NOP)

#MM
setup-clib-pc-i386 :
	%mkdirs_q $(CLIBDIR)
	%add_objects $(foreach f,$(FILES), compiler/clib/$(f))

#MM
clean ::
	-$(RM) $(TESTS)

$(CLIBDIR)/%.o : %.c
	%compile_q opt=$(CFLAGS2)

$(CLIBDIR)/%.o : %.s
	%assemble_q opt=$(AFLAGS2)

$(CLIBDIR)/%.d : %.c
	%mkdepend_q

%common
