# $Id$
include $(TOP)/config/make.cfg

OBJDIR := $(OSGENDIR)/dos
OS_INCLUDES := -I$(INCDIR)
CFLAGS2 := $(CFLAGS) -I.. -I$(TOP)/rom/dos
AFLAGS2 :=
TESTS :=

FILES := aros dosboot boot

#MM
dos-pp-m68k : $(foreach f, $(FILES), $(OBJDIR)/$(f).o)
	@$(NOP)

#MM
setup-dos-pp-m68k :
	%mkdirs_q $(OBJDIR)
	%add_objects $(foreach f,$(FILES), rom/dos/$(f))
	%exclude_files $(foreach f, $(FILES), rom/dos/$(f))

#MM
clean ::
	-$(RM) $(TESTS)

$(OBJDIR)/%.o : %.c
	%compile_q opt="$(CFLAGS2) -Dmain=submain"

$(OBJDIR)/%.d : %.c
	%mkdepend_q

%common
