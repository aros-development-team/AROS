#MM- kernel-usb-shelltools : kernel-usb-poseidon-includes

#MM- kernel-usb-usbromstartup : kernel-usb-poseidon

%build_module mmake=kernel-usb-poseidon \
    modname=poseidon modtype=library \
    files="$(FUNCS) $(FILES)" \
