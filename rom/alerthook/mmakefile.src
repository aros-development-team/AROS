include $(TOP)/config/make.cfg

#MM kernel-alerthook-module : includes linklibs kernel-alerthook-setup
#MM kernel-alerthook-setup
kernel-alerthook-module : $(AROS_MODULES)/alert.hook

%rule_makedirs dirs=$(GENDIR)/$(CURDIR) setuptarget=kernel-alerthook-setup

%rule_compile basename=% targetdir=$(GENDIR)/$(CURDIR)

$(AROS_MODULES)/alert.hook : $(OBJDIR)/alerthook.o
	%link_module_q objs=$^

%include_deps deps=$(GENDIR)/$(CURDIR)/alerthook.d

%common
