include $(TOP)/config/make.cfg

#MM kernel-battclock : setup includes kernel-battclock-$(ARCH)-$(CPU)
#MM kernel-battclock-linklib : setup includes kernel-battclock-$(ARCH)-$(CPU)

FUNCTIONS := readbattclock resetbattclock writebattclock

%build_module_macro mmake=kernel-battclock \
  modname=battclock modtype=resource \
  funcs=$(FUNCTIONS) \
  conffile=lib.conf \
  genshlinklib=$(SHARED_BATTCLOCK) \
  genincludes=yes

%common
