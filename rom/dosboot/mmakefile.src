include $(TOP)/config/make.cfg

FILES := dosboot_init inithidds boot menu gadgets bootconfig

#MM- kernel-dosboot-includes : kernel-bootloader-includes
#MM- kernel-dosboot : includes linklibs kernel-bootloader-includes

USER_INCLUDES := -I$(SRCDIR)/rom/devs
USER_CFLAGS := -DAROS_ARCHITECTURE="\"$(AROS_TARGET_ARCH)-$(AROS_TARGET_CPU)\""

%build_module mmake=kernel-dosboot \
  modname=dosboot modtype=resource \
  files=$(FILES) uselibs="rom oop hiddstubs"
