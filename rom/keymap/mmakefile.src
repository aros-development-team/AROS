include $(TOP)/config/make.cfg

FILES := defaultkeymap support
FUNCS := askkeymapdefault mapansi maprawkey setkeymapdefault

#MM- kernel-keymap-linklib : kernel-keymap-intern-linklib

#MM- kernel-keymap-intern-linklib : includes linklibs

%build_module_macro mmake=kernel-keymap-intern \
  modname=keymap modtype=library \
  funcs=$(FUNCS) files=$(FILES) \
  conffile=lib.conf \
  genshlinklib=$(SHARED_KEYMAP)

%common
