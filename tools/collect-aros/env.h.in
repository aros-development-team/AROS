#ifndef _ENV_H_
#define _ENV_H_

#ifdef _NATIVE_
#define LD_NAME "/Development/bin/ld"
#define STRIP_NAME "/Development/bin/strip"
#endif
/* Under Windows we can't use paths provided by configure because they are UNIX-style and we don't
   use MSYS */
#ifdef _WIN32
#define LD_NAME "@aros_target_cpu@-aros-ld"
#define STRIP_NAME "@aros_target_cpu@-aros-strip"
#endif
#ifndef LD_NAME
#define LD_NAME "@orig_target_ld@"
#define STRIP_NAME "@aros_target_strip@"
#endif
#define OBJECT_FORMAT "-m@aros_object_format@"

#endif /* !_ENV_H */
