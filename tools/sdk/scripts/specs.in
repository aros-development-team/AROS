*cpp:
%(cpp_cpu) %{fPIC:-D__PIC__ -D__pic__} %{fpic:-D__PIC__ -D__pic__} %{posix:-D_POSIX_SOURCE} %{pthread:-D_REENTRANT} -nostdinc %{!nostdinc:-isystem @aros_sdk_path@/@cc_cpu@-aros/include -isystem @cc_include@}

*endfile:
-lautoinit

*link:
-m @ld_emu@ -L@aros_sdk_path@/@cc_cpu@-aros/lib

