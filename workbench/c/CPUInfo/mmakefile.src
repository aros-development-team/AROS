include $(TOP)/config/make.cfg

FILES	:= main 

x86FILES := x86/main \
            x86/AMD/main \
            x86/Cyrix/main \
            x86/Intel/main \
            x86/NatSemi/main \
            x86/RiSE/main

EXEDIR  := $(AROS_C)

#MM workbench-c-cpuinfo : includes linklibs kernel-cpu-pc-i386

USER_LDFLAGS := -nostdc

%build_prog mmake=workbench-c-cpuinfo \
    progname=CPUInfo targetdir=$(EXEDIR) \
    files="$(FILES) $(x86FILES)"

%common
