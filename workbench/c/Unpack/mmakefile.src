# Makefile to build Unpack program.

include $(TOP)/config/make.cfg

FILES 	    := unpack gui package bzip2 file support
EXEDIR      := $(AROS_C)
EXE 	    := $(EXEDIR)/Unpack

USE_AMIGALIB    := yes
NO_AUTOINIT := yes

#MM- workbench-c : workbench-c-unpack
#MM workbench-c-unpack : includes linklibs-bz2

%make_prog_setup mmake=workbench-c-unpack startup=

LIBS := -lbz2 $(LIBS)

workbench-c-unpack : setup $(EXE)

%make_prog_multi dir=$(EXEDIR) target=$(EXE)

%common
%include_deps
