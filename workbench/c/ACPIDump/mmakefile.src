include $(TOP)/config/make.cfg

FILES	:= acpidump

x86FILES := x86/acpidump \
            x86/AMD/acpidump \
            x86/Cyrix/acpidump \
            x86/Intel/acpidump \
            x86/NatSemi/acpidump \
            x86/RiSE/acpidump

EXEDIR  := $(AROS_C)

#MM workbench-c-acpidump : includes linklibs

%build_prog mmake=workbench-c-acpidump \
    progname=ACPIDump targetdir=$(EXEDIR) \
    files="$(FILES) $(x86FILES)" \
    uselibs="arossupport amiga arosc m"

%common
