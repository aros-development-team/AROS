CCFLAGS = gst=/libs/muimaster/all.gst code=far noversion IDIR=/libs/muimaster/include IDIR=/libs/muimaster

zune: prefsio.o zune.o zunestuff.o /libs/muimaster/muimaster.lib
  sc link zune.o zunestuff.o /libs/muimaster/muimaster.lib lib:debug.lib

prefsio.o: prefsio.c
  sc $(CCFLAGS) prefsio.c

zune.o: zune.c
  sc $(CCFLAGS) zune.c

zunestuff.o: zunestuff.c
  sc $(CCFLAGS) zunestuff.c

clean:
  delete zune zune.o

