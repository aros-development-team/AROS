include $(SRCDIR)/config/aros.cfg

#MM classes-zune-nlisttree-class : includes linklibs

USER_INCLUDES = -idirafter $(SRCDIR)/$(CURDIR)/../include
USER_CPPFLAGS := -DNO_INLINE_STDARG -DAROS_ABI_V1
USER_CFLAGS += $(PARANOIA_CFLAGS)
