FILES := library \
         ClipboardServer \
         NListtree \
         QuickSort \
         Debug

%build_module_simple mmake=classes-zune-nlisttree-class \
  modname=NListtree modtype=mcc \
  files=$(FILES)


#MM includes-copy

INCLUDE_FILES := ../include/mui/NListtree_mcc.h
%copy_includes path=mui dir=../include/mui

%common
