LIBOBJS = libfunc.o pictureclass.o colorhandling.o

picture.datatype: ${LIBOBJS}
   slink with <<
LIBFD FD:dtclass_lib.fd
to picture.datatype
FROM lib:libent.o lib:libinit.o $(LIBOBJS)
lib lib:sc.lib
lib lib:debug.lib
lib lib:amiga.lib
NOICONS
SD
SC
libversion 41 librevision 0 libid "picture.datatype 41.0 (03-Oct-2000)"
<
   avail flush

libfunc.o: libfunc.c
   sc nostackcheck libcode define=MYDEBUG libfunc.c OBJNAME=$@

pictureclass.o: pictureclass.c
   sc nostackcheck define=MYDEBUG pictureclass.c OBJNAME=$@

colorhandling.o: colorhandling.c
   sc nostackcheck define=MYDEBUG colorhandling.c OBJNAME=$@

clean:
    delete FORCE libfunc.o
    delete FORCE pictureclass.o
    delete FORCE picture.datatype

