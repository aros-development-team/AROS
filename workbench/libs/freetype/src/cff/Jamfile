# FreeType 2 src/cff Jamfile (c) 2001 David Turner
#

SubDir  FT2_TOP src cff ;

SubDirHdrs  [ FT2_SubDir  src cff ] ;

{
  local  _sources ;

  if $(FT2_MULTI)
  {
    _sources = cffdrivr cffgload cffload cffobjs cffparse ;
  }
  else
  {
    _sources = cff ;
  }

  Library  $(FT2_LIB) : $(_sources).c ;
}

# end of src/cff Jamfile
