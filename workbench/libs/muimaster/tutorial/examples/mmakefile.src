# $Id$
#
# Makefile for Zune step by step examples

include $(TOP)/config/make.cfg

FILES  := \
    HelloZune \
    HGroup    \
    VGroup    \
    VHGroup   \
    VHGroup2  \
    HVGroup   \
    Notify

EXEDIR := $(AROSDIR)/Demos/Zune
EXES   := $(foreach f,$(FILES),$(EXEDIR)/$(f))

USE_CLIB := yes
USE_AMIGALIB    := yes
USE_MUILIB      := yes
USE_AROSLIB     := yes

#MM workbench-libs-muimaster-examples : \
#MM   workbench-libs-muimaster-examples-setup includes linklibs

%make_prog_setup mmake=workbench-libs-muimaster-examples

workbench-libs-muimaster-examples : $(EXES)

#MM
workbench-libs-muimaster-examples-quick : $(EXES)

%make_prog_single dir=$(EXEDIR)

#MM workbench-libs-muimaster-examples-setup
%rule_makedirs dirs=$(EXEDIR) setuptarget=workbench-libs-muimaster-examples-setup

%common
%include_deps
