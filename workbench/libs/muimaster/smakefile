  sc $(CFLAGS) $(LIBSDIRS) libcode mui_requestidcmp.c objname=$@

objs/mui_seterror.o: mui_seterror.c
  sc $(CFLAGS) $(LIBSDIRS) libcode mui_seterror.c objname=$@

clean:
  -delete objs all
