include $(SRCDIR)/config/aros.cfg

USER_LDFLAGS := -static

FUNCS := \
	CreateVLayerHandleTagList \
	DeleteVLayerHandle \
	AttachVLayerTagList \
	DetachVLayer \
	GetVLayerAttr \
	LockVLayer \
	UnLockVLayer \
	SetVLayerAttrTagList

#MM- workbench-libs-cgxvideo : linklibs
