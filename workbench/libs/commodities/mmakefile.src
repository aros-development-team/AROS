include $(TOP)/config/make.cfg

ifeq ($(TARGET_ARCH),morphos)
OS_INCLUDES 	:= -I$(AROSDIR)/Include -I/ade/morphos/emulinclude/includegcc -I/ade/morphos/emulinclude/includestd -I/ade/morphos/include
FORCE_DEPLIBS 	:=
FORCE_LIBS 		:= /ade/morphos/lib/libc.a /ade/morphos/lib/libsyscall.a
endif

FILES := commodities_inputhandler eventfuncs

FUNCS := activatecxobj \
	 addievents \
	 attachcxobj \
	 clearcxobjerror \
	 createcxobj \
	 cxbroker \
	 cxmsgdata \
	 cxmsgid \
	 cxmsgtype \
	 cxnotify \
	 cxobjerror \
	 cxobjtype \
	 deletecxobj \
	 deletecxobjall \
	 disposecxmsg \
	 divertcxmsg \
	 enqueuecxobj \
	 freebrokerlist \
	 getbrokerlist \
	 insertcxobj \
	 invertkeymap \
	 matchix \
	 parseix \
	 removecxobj \
	 routecxmsg \
	 setcxobjpri \
	 setfilter \
	 setfilterix \
	 settranslate

#MM- workbench-libs-commodities : includes linklibs workbench-libs-commodities-module

%build_module_macro mmake=workbench-libs-commodities modname=commodities \
  modtype=library funcs=$(FUNCS) files=$(FILES) uselibs="arossupport rom" \
  conffile=lib.conf

%common
