#   $Id$
#
#   Makefile for the OpenURL catalogs of openurl.library

include $(TOP)/config/make.cfg

#MM
external-openurl-catalogs : \
    $(AROS_CATALOGS)/german/OpenURL.catalog \
    $(AROS_CATALOGS)/french/OpenURL.catalog \
    $(AROS_CATALOGS)/polish/OpenURL.catalog

# Catalogs which aren't uptodate:
#$(AROS_CATALOGS)/swedish/OpenURL.catalog
#$(AROS_CATALOGS)/italian/OpenURL.catalog

$(AROS_CATALOGS)/german/OpenURL.catalog  : german.ct
	@$(MKDIR) $(AROS_CATALOGS)/german
	@$(FLEXCAT) $(SRCDIR)/$(CURDIR)/OpenURL.cd $< CATALOG=$@

$(AROS_CATALOGS)/italian/OpenURL.catalog : italian.ct
	@$(MKDIR) $(AROS_CATALOGS)/italian
	@$(FLEXCAT) $(SRCDIR)/$(CURDIR)/OpenURL.cd $< CATALOG=$@

$(AROS_CATALOGS)/swedish/OpenURL.catalog  : swedish.ct
	@$(MKDIR) $(AROS_CATALOGS)/swedish
	@$(FLEXCAT) $(SRCDIR)/$(CURDIR)/OpenURL.cd $< CATALOG=$@

$(AROS_CATALOGS)/french/OpenURL.catalog  : french.ct
	@$(MKDIR) $(AROS_CATALOGS)/french
	@$(FLEXCAT) $(SRCDIR)/$(CURDIR)/OpenURL.cd $< CATALOG=$@

$(AROS_CATALOGS)/polish/OpenURL.catalog  : polish.ct
	@$(MKDIR) $(AROS_CATALOGS)/polish
	@$(FLEXCAT) $(SRCDIR)/$(CURDIR)/OpenURL.cd $< CATALOG=$@

%common
